User = struct {
    name: string,
    age in 2..: enum { age: number, unknown },
    contact: versioned {
        ..3: string,
        4: Contact,
        5..: [Contact],
    },
    is_admin in 6..: bool,
};

Contact = enum {
    phone: number,
    email: string,
    address in 5..: struct {
        street: string,
        city: string,
        country: string,
    },
};
